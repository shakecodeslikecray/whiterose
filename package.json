{
  "name": "@shakecodeslikecray/whiterose",
  "version": "2.0.1",
  "description": "AI-powered bug hunter that uses your existing LLM subscription. \"I've been staring at your code for a long time.\"",
  "keywords": [
    "bug-finder",
    "static-analysis",
    "ai",
    "llm",
    "code-quality",
    "security",
    "cli"
  ],
  "author": "shakecodeslikecray",
  "license": "PolyForm-Noncommercial-1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/shakecodeslikecray/whiterose"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "whiterose": "dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsup",
    "start": "node dist/cli/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ollama": "^0.6.3",
    "openai": "^6.17.0",
    "react": "^18.3.1",
    "typescript": "^5.5.3",
    "yaml": "^2.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^8.57.0",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "vitest": "^2.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
